#!/bin/sh

cd /opt/bin
case "$1" in
'') bash -x "appman" ;;
'anydesk') wget https://raw.githubusercontent.com/ivan-hc/AppMan/main/applications/anydesk/anydesk-installer ; chmod a+x ./anydesk-installer ; exec /opt/bin/anydesk-installer ;;
'ice') wget https://raw.githubusercontent.com/ivan-hc/AppMan/main/applications/ice/ice-installer ; chmod a+x ./ice-installer ; exec /opt/bin/ice-installer ;;
'ocenaudio') wget https://raw.githubusercontent.com/ivan-hc/AppMan/main/applications/ocenaudio/ocenaudio-installer ; chmod a+x ./ocenaudio-installer ; exec /opt/bin/ocenaudio-installer ;;
'openshot') wget https://raw.githubusercontent.com/ivan-hc/AppMan/main/applications/openshot/openshot-installer ;	chmod a+x ./openshot-installer ; exec /opt/bin/openshot-installer ;;
'qbittorrent') wget https://raw.githubusercontent.com/ivan-hc/AppMan/main/applications/qbittorrent/qbittorrent-installer ; chmod a+x ./qbittorrent-installer ; exec /opt/bin/qbittorrent-installer ;;
*) exec /opt/bin/appman-install ;;
esac
